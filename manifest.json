{
  "manifest_version": 3,
  "name": "Algebra-Geometry",
  "description": "Learn Algebra and Geometry!",
  "version": "1.0",
  "action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "permissions": ["tabs", "host_permissions"],
  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["main.js", "popup.js"]
      }
  ]
}